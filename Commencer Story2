		set {Commence::%{_p}%::Skill::CraftingLevel} to 0 if {Commence::%{_p}%::Skill::CraftingLevel} is not set
		set {Commence::%{_p}%::Skill::MagicLevel} to 0 if {Commence::%{_p}%::Skill::MagicLevel} is not set
