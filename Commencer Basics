	#SERVER CONDITIONS

every 5 seconds:
	loop all players:
		add 5 to {Commence::%loop-player%::Playtime} and {Commence::%loop-player%::Playtime::Current}
		rankCheck(loop-player)

#-=-=-=-=-=-=-=-=-=-
	#SIDEBAR

function sideBar(p: player):
	wipe {_p}'s sidebar
	{_p}'s world is "Survival2":
		{Commence::%{_p}%::LiferBar} is not set:
			set {Commence::%{_p}%::LiferBar} to true
		{Commence::%{_p}%::LiferBar} is not true:
			toggle {_p}'s scoreboard to off
			stop
		set {_t} to "Generating..."
		set title of {_p}'s scoreboard to "<##70797C> -&k| {@lifer} &k|<##70797C>-"
		set line 7 of {_p}'s scoreboard to "&7 "
		set line 6 of {_p}'s scoreboard to "&7 <##57D1F9>%{Lifer::%{_p}%::First} ? {_p}'s display name%<##00C889>'s <##00C08D>I<##00BA8D>n<##00B48D>f<##00AE8D>o:"
		set line 5 of {_p}'s scoreboard to "&7  <##00C889>F<##00C08D>a<##00BA8D>m<##00B48D>i<##00AE8D>l<##00A88D>y: <##8B969B>%{Lifer::%{_p}%::Last} ? {_t}%"
		set line 4 of {_p}'s scoreboard to "&7   <##00C889>A<##00C08D>g<##00BA8D>e: <##8B969B>%{Lifer::%{_p}%::Age} ? {_t}%"
		uncolored {Lifer::%{_p}%::Parent}'s display name is not set:
			set {Lifer::%{_p}%::Parent} to "Unknown"
		set line 3 of {_p}'s scoreboard to "&7  <##00C889>P<##00C08D>a<##00BA8D>r<##00B48D>e<##00AE8D>n<##00A88D>t: <##8B969B>%{Lifer::%{_p}%::Parent}'s display name ? {Lifer::%{_p}%::Parent}%" 
		set line 2 of {_p}'s scoreboard to "&7   <##00C889>F<##00C08D>o<##00BA8D>u<##00B48D>n<##00AE8D>d<##00A88D>e<##00A28D>r: <##8B969B>%{Lifer::%{_p}%::Founder} ? {Lifer::%{_p}%::Founder}'s display name ? {Lifer::%{_p}%::Parent}%"
		set line 1 of {_p}'s scoreboard to "&7 "
		clear {_t}
	else if {_p}'s world is not "Survival3":
		{Commence::%{_p}%::SideBar} is not set:
			set {Commence::%{_p}%::SideBar} to true
		{Commence::%{_p}%::SideBar} is not true:
			toggle {_p}'s scoreboard to off
			stop
		set title of {_p}'s scoreboard to "<##70797C> -&k| {@commencesmp} &k|<##70797C>-"
		set line 13 of {_p}'s scoreboard to "&7 "
		set line 12 of {_p}'s scoreboard to "&7 <##57D1F9>%{_p}'s display name%<##50C0E5>'s <##49AFD1>I<##429EBC>n<##3988A3>f<##307389>o:"
		set line 11 of {_p}'s scoreboard to "&7  <##57D1F9>R<##50C0E5>a<##49AFD1>n<##429EBC>k: <##8B969B>%{Commence::%{_p}%::Rank}%"
		set {Commence::%{_p}%::Funds} to balance of {_p}
		set line 10 of {_p}'s scoreboard to "&7   <##57D1F9>M<##50C0E5>o<##49AFD1>n<##429EBC>e<##3988A3>y: <##8B969B>%{Commence::%{_p}%::Funds}%"
		set line 9 of {_p}'s scoreboard to "&7  <##57D1F9>D<##50C0E5>e<##49AFD1>a<##429EBC>t<##3988A3>h<##307389>s: <##8B969B>%{Survival::%{_p}%::Deaths} ? 0%"
		set line 8 of {_p}'s scoreboard to "&7 "
		set line 7 of {_p}'s scoreboard to "&7 <##57D1F9>P<##50C0E5>l<##49AFD1>a<##429EBC>y<##3988A3>e<##307389>r<##296275>s:"
		set line 6 of {_p}'s scoreboard to "&7  <##57D1F9>T<##50C0E5>o<##49AFD1>t<##429EBC>a<##3988A3>l: <##8B969B>%amount of all players%"
		set line 5 of {_p}'s scoreboard to "&7   {@mythicalrealms}: <##8B969B>%amount of all players in world("Survival3")%"
		set line 4 of {_p}'s scoreboard to "&7  &fOGSurvival: <##8B969B>%amount of all players in world("Survival")%"
		set line 3 of {_p}'s scoreboard to "&7   <##529E6A>Lifer: <##8B969B>%amount of all players in world("Survival2")%"
		set line 2 of {_p}'s scoreboard to "&7 "
#		set line 1 of {_p}'s scoreboard to "&7 "
	else:
		{Commence::%{_p}%::StoryBar} is not set:
			set {Commence::%{_p}%::StoryBar} to true
		else if {Commence::%{_p}%::Story::Form} is not set:
			toggle {_p}'s scoreboard to off
			stop
		else if {Commence::%{_p}%::StoryBar} is not true:
			toggle {_p}'s scoreboard to off
			stop
		stop if {Commence::%{_p}%::StoryBar} is not true
		set title of {_p}'s scoreboard to "<##70797C> -&k|&r {@mythicalrealms} &k<##70797C>|<##70797C>-"
		set line 13 of {_p}'s scoreboard to "&7 "
		set line 12 of {_p}'s scoreboard to "&7  <##855EC9>🥛:%{Commence::%{_p}%::Story::Mana} ? 0% <##00C889>⌛:%{_p}'s walk speed * 10 ? 0% <##00608D>●:%{Commence::%{_p}%::Story::Level} ? 0% %{Commence::%{_p}%::Story::Experience} ? 0%/%{Commence::%{_p}%::Story::LevelUpgrade} ? 0%"
		set line 11 of {_p}'s scoreboard to "&7   <##CB625F>🗡:%{Commence::%{_p}%::Story::Attack} ? 0% <##CB625F>⚔:%({Commence::%{_p}%::Story::Defense} ? 0) * -1% <##CB625F>☠:%{Survival::%{_p}%::Deaths} ? 0%"
		set line 10 of {_p}'s scoreboard to "&7  <##D0944D>✳:%{Commence::%{_p}%::Quest::Score} ? 0% <##D0944D>❇:%{Commence::%{_p}%::Story::Position} ? 0% <##D0944D>₵:%balance of {_p}%"
		set line 9 of {_p}'s scoreboard to ""
		{Commence::%{_p}%::Story::Position} is less than 1:
			set {Commence::%{_p}%::Story::Position} to 1
		set line 8 of {_p}'s scoreboard to "&7  <##D0944D>%{Commence::%{_p}%::DisplayName}%"
		set line 7 of {_p}'s scoreboard to "&7   <##FFC889>F<##FCB35F>o<##D0944D>r<##A0723D>m: <##D0944D>%{Commence::%{_p}%::Story::Form}%"
		set line 6 of {_p}'s scoreboard to "&7  <##FFC889>R<##FCB35F>a<##D0944D>n<##A0723D>k: <##D0944D>%{Commence::%{_p}%::Story::Rank}%"
		set line 5 of {_p}'s scoreboard to "&7   <##FFC889>K<##FFC17A>i<##FCB35F>n<##E2A156>g<##A0723D>d<##8C6335>o<##77552D>m: <##D0944D>%{Commence::%{_p}%::Story::Kingdom}%"
		set line 4 of {_p}'s scoreboard to "&7  <##FFC889>J<##FCB35F>o<##D0944D>b: <##D0944D>%{Commence::%{_p}%::Story::Job}%"
		set line 3 of {_p}'s scoreboard to "&7   <##FFC889>Q<##FCB35F>u<##D0944D>e<##A0723D>s<##77552D>t: <##D0944D>%{Commence::%{_p}%::Quest::Active}%"
		{Commence::%{_p}%::Quest::Progress} >= {Commence::%{_p}%::Quest::Total}:
			set line 2 of {_p}'s scoreboard to "&7  <##FFC889>P<##FFC17A>r<##FCB35F>o<##E2A156>g<##A0723D>r<##8C6335>e<##77552D>s<##77552D>s: %{Commence::%{_p}%::Quest::Proper}%"
			set {_tm} to random element of {Commence::FindNPC::*}
			loop 6 times:
				clear {_dist}
				loop all entities in radius 200 of {_p}:
					display name of loop-entity is {Commence::%{_p}%::NPC::Active}:
						set {_dist} to round(distance between loop-entity and {_p})
						"%direction from {_p} to loop-entity%" contains "above":
							set {_tl} to "up"
						else if "%direction from {_p} to loop-entity%" contains "below":
							set {_tl} to "down"
						else:
							set {_tl} to "away"
						set {_loc} to "%{_tl}%"
						stop loop
				if {_dist} is not set:
					set {_dist} to "More than 199"
				if {_loc} is not set:
					set {_loc} to "away"
				{Commence::%{_p}%::NPC::Active} is set:
					set action bar of {_p} to "&7  %{_tm}%%{Commence::%{_p}%::NPC::Active}%&7 (%{_dist}% blocks %{_loc}%)"
				wait 1 second
		else if {Commence::%{_p}%::Quest::Total} is an integer:
			set line 2 of {_p}'s scoreboard to "&7  <##FFC889>P<##FFC17A>r<##FCB35F>o<##E2A156>g<##A0723D>r<##8C6335>e<##77552D>s<##77552D>s: <##D0944D>%{Commence::%{_p}%::Quest::Progress}%/%{Commence::%{_p}%::Quest::Total}%"
		else if {Commence::%{_p}%::Quest::Total} is not an integer:
			set line 2 of {_p}'s scoreboard to "&7  <##FFC889>P<##FFC17A>l<##FCB35F>a<##E2A156>y<##A0723D>e<##8C6335>r<##77552D>s: <##D0944D>%amount of all players in world("Survival3")%"
		set line 1 of {_p}'s scoreboard to "&7 "
	toggle {_p}'s scoreboard to on

#-=-=-=-=-=-=-=-=-=-
	#SPAWNHUB PARKOUR

on walking on barrier:
	world is "SpawnHub":
		{Commence::%player%::AdminEdit} is true:
			stop
		{SpawnHub::Parkour1S} is not set:
			teleport player to {Spawn::SpawnHub}
			stop
		{SpawnHub::%player%::Parkour2} is set:
			teleport player to {SpawnHub::Parkour2S}
			stop
		teleport player to {SpawnHub::Parkour1S}
		stop

#-=-=-=-=-=-=-=-=-=-
	#BLOCK RULES

on build:
	world is "SpawnHub":
		{Commence::%player%::AdminEdit} is true:
			stop
		cancel event

on break:
	world is "SpawnHub":
		{Commence::%player%::AdminEdit} is true:
			stop
		cancel event

on break of bedrock:
    cancel event

on place of bedrock:
    cancel event

#-=-=-=-=-=-=-=-=-=-
	#SERVER NAVIGATION

on click:
	player's world is "SpawnHub":
		uncolored name of player's tool contains "Navigator":
			execute player command "/c"

command /randomteleport [<player>]:
	aliases: /rtp, /wild, /wilderness, /tpr
	usage: &c /randomteleport
	description: &7 Teleports to a random location.
	cooldown: 5 minutes
	cooldown bypass: *
	cooldown message: &9 Commence&8SMP &c There's a 5 minute cooldown to use this again!
	trigger:
		if argument isn't set:
			player's world is "Survival3" or "SpawnHub":
				set {Commence::%player%::Story::Wait} to true
				send ""
				send "{@rtp}"
				send "&c   You are not in the right world" if player's world is not "Survival3"
				send "" if player's world is not "Survival3"
#				teleport player to {Spawn::Survival3} if player's world is not "Survival3"
				send "&7   Now teleporting to random location in &6{@mythicalrealms}..."
				send "&7   This could take 5 to 10 seconds..."
				set {_t1} to random integer between -5000 and 5000
				set {_t2} to random integer between 120 and 200
				set {_t3} to random integer between -5000 and 5000
				spawn armor stand at player's location with nbt compound from "{Invisible:1b}"
				set name of last spawned armor stand to "%player%'s RTP"
				set {Commence::%player%::RTPTest} to last spawned entity
				teleport {Commence::%player%::RTPTest} to location {_t1}, {_t2}, {_t3} in world "Survival3"
#				wait 1 second
				clear {Commence::%player%::Story::Wait}
				block under {Commence::%player%::RTPTest} is air:
					loop all blocks below {Commence::%player%::RTPTest}:
						loop-block is water or lava:
							teleport {Commence::%player%::RTPTest} to location of block in front of loop-block
						else if loop-block is air:
							teleport {Commence::%player%::RTPTest} to location of loop-block
						else if loop-block is not a grass block:
							teleport {Commence::%player%::RTPTest} to location of block above loop-block
						else:
							stop loop
				block at {Commence::%player%::RTPTest} is not air:
					loop all blocks below {Commence::%player%::RTPTest}:
						loop-block is not air:
							teleport {Commence::%player%::RTPTest} to location of block above loop-block
							block under {Commence::%player%::RTPTest} is grass block:
								set {Commence::%player%::RTP} to location of {Commence::%player%::RTPTest}
								teleport player to {Commence::%player%::RTP}
								stop loop
				else:
					block under {Commence::%player%::RTPTest} is grass block:
						set {Commence::%player%::RTP} to location of {Commence::%player%::RTPTest}
						teleport player to {Commence::%player%::RTP}
					else:
						set {Commence::%player%::RTP} to location of {Commence::%player%::RTPTest}
						teleport player to {Commence::%player%::RTP}
				set {Story::%player%::LastLoc} to location of {Commence::%player%::RTP}
				kill all entities in radius 50 of player where [name of entity input is "%player%'s RTP"]
				clear {Commence::%player%::RTPTest}
				clear {Commence::%player%::RTP}
				send "%random element of {Survival::RTPMsg::*}%"
				send "&7    and good luck &8%player's display name%&7!"
			else if player's world is "Survival2":
				send ""
				send "{@rtp}"
				send "&c   You are not permitted to RTP in this gamemode"
				stop
			else:
				set {Commence::%player%::Story::Wait} to true
				send ""
				send "{@rtp}"
				send "&c   You are not in the right world" if player's world is not "Survival"
				send "" if player's world is not "Survival"
#				teleport player to {Spawn::Survival} if player's world is not "Survival"
				send "&7   Now teleporting to random location in &fOGSurvival::.."
				set {_t1} to random integer between -5000 and 5000
				set {_t2} to random integer between 150 and 250
				set {_t3} to random integer between -5000 and 5000
				teleport player to location {_t1}, {_t2}, {_t3} in world "Survival"
#				wait 1 second
				block at player is not air:
					loop all blocks in radius 100 of player:
						loop-block is air:
							teleport player to location of loop-block
							block under player is not air:
								stop loop
				block under player is air:
					loop all blocks under player:
						loop-block is water:
							stop loop
						else if loop-block is air:
							teleport player to location of loop-block
						else:
							stop loop
				send "%random element of {Survival::RTPMsg::*}%"
				send "&7    and good luck &8%player's display name%&7!"
				clear {Commence::%player%::Story::Wait}

command /spawn [<text>]:
	aliases: s, hub, lobby
	usage: &c /spawn [ WORLD ]
	description: &7 Teleports to the server spawn.
	cooldown: 2 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		stop if {Commence::%player%::Wait} is true
		stop if {Commence::%player%::StoryWait} is true
		set {Commence::%player%::Wait} to false
		set {Commence::%player%::StoryWait} to false
		player's world is "Survival3":
			set {Story::%player%::LastLoc} to player's location
			resetStory(player)
		player's world is "Survival2":
			{Lifer::%player%::Condition} is true:
				resetLifer(player)
		player's world is "Survival":
			set {Survival::%player%::LastLoc} to player's location
		if argument is not set:
			teleport player to {Spawn::SpawnHub}
			wait 1 tick
			set slot 1 and 2 and 3 and 5 and 6 and 7 and 8 and 9 of player to air
			set slot 4 of player to compass named "&8&l  >>  <##3988A4>C<##3C91AD>o<##4099B7>m<##43A2C1>m<##49AFD1>e<##4EBBE0>n<##52C4EA>c<##55CDF4>e<##545B5E>Navigator &8&l  <<  " with lore "&7     Click to View Gamemodes    &7"
			send ""
			send "{@commence}"
			send "{@tpmsg}"
			stop
		else:
			argument contains "survival3" or "story":
				teleport player to {Spawn::Survival3}
				send ""
				send "{@commence}"
				send "{@tpmsgs}"
				stop
			argument contains "survival" or "ogsurvival":
				teleport player to {Spawn::Survival}
				send ""
				send "{@commence}"
				send "{@tpmsgs}"
				stop
			{Spawn::%argument%} is set:
				teleport player to {Spawn::%argument%}
				send ""
				send "{@commence}"
				send "{@tpmsg}"
				stop
			else:
				send ""
				send "{@commence}"
				send "&c   %arg-1%&c is not a world or gamemode"
				send "&c    Type &9/c&c to view gamemodes"
				stop

command /survival:
	aliases: /ogsurvival, /smp
	usage: &c survival
	description: &7 Teleports to the Survival
	cooldown: 2 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		{Survival::Enabled} is not true:
			send ""
			send "{@commence}"
			send "&c   Sorry, this game mode is currently disabled"
			send "&7   To try other game modes, use &9/c"
			stop
		player's world is "Survival":
			send ""
			send "{@commence}"
			send "&c   You're already in &fOGSurvival"
			send "&7   To teleport to &9Survivalspawn&7, use &9/spawn survival"
			stop
		else if player's world contains "PVP":
			send ""
			send "{@commence}"
			send "&c   To make the KitPvP experience better,"
			send "&c    this command has been disabled"
			stop
		else if player's world is "Survival2":
			execute player command "/s"
		set walk speed of player to 0.2
		set fly speed of player to 0.2
		set the maximum health of player to 10
		set player's flight mode to false
		set {Commence::%player%::Wait} to false
		{PVP::%player%::PVP} is true:
			remove player from {PVP::AllPlayers::*}
			clear {PVP::%player%::PVP}
			clear {PVP::%player%::PVE}
			remove 1 from {PVP::AllPlayerCount}
		execute console command "/sogs %player%"

on inventory click:
	if event-inventory is (metadata tag "warps" of player):
		cancel event
		set {Commence::List} to 9
		loop {Warp::Warps::*} and {Warp::Warps.S::*}:
			add 1 to {Commence::List}
			{Commence::List} is 17:
				add 2 to {Commence::List}
			else if {Commence::List} is 26:
				add 2 to {Commence::List}
			else if {Commence::List} is 35:
				add 2 to {Commence::List}
			index of event-slot is {Commence::List}:
				close inventory of player
				teleport player to {Warp::%loop-value%::TP}
				send ""
				send "{@commence}"
				send "&7   You warped to %{Warp::%loop-value%}%"
				stop
		index of event-slot is 8:
			close inventory of player
	if event-inventory is (metadata tag "homes" of player):
		cancel event
		set {Commence::List} to 0
		loop {Commence::%player%::Homes::All::*}:
			add 1 to {Commence::List}
			set {_th} to uncolored loop-value
		if {Commence::List} is more than 21:
			set {_t} to 6
		else if {Commence::List} is more than 14:
			set {_t} to 5
		else if {Commence::List} is more than 7:
			set {_t} to 4
		else:
			set {_t} to 3
		index of event-slot is 12:
			close inventory of player
			set {Commence::%player%::Homes::%{_th}%::TP} to player's location
			send ""
			send "{@storyhomes}"
			send "<##00C889>   You changed location for home %{Commence::%player%::Homes::%{_th}%}%"
			send "<##00C889>    to %{Commence::%player%::Homes::%{_th}%::TP}%"
			stop
		index of event-slot is 13:
			close inventory of player
			{Commence::List} is less than 1:
				set {_th} to "Home"
				set {Commence::%player%::Homes::%{_th}%::TP} to player's location
				set {Commence::%player%::Homes::%{_th}%} to "<##D0944D>%{_th}%"
				add {Commence::%player%::Homes::%{_th}%} to {Commence::%player%::Homes::All::*}
				send ""
				send "{@storyhomes}"
				send "<##00C889>   You set a new home called %{Commence::%player%::Homes::%{_th}%}%"
				send "<##00C889>    at %{Commence::%player%::Homes::%{_th}%::TP}%"
				stop
			else:
				teleport player to {Commence::%player%::Homes::%{_th}%::TP}
				send ""
				send "{@storyhomes}"
				send "&7   You teleported to home %{Commence::%player%::Homes::%{_th}%}%"
				stop
			wait 1 tick
			execute player command "/home"
		index of event-slot is 14:
			close inventory of player
			send ""
			send "{@storyhomes}"
			send "<##CB625F>   You removed home %{Commence::%player%::Homes::%{_th}%}%"
#			clear {Commence::%player%::Homes::*}
			remove {Commence::%player%::Homes::%{_th}%} from {Commence::%player%::Homes::*} and {Commence::%player%::Homes::All::*}
			clear {Commence::%player%::Homes::%{_th}%}
			clear {Commence::%player%::Homes::%{_th}%::TP}
			stop
		index of event-slot is 8:
			close inventory of player
	if event-inventory is (metadata tag "kingdoms" of player):
		cancel event
		{Commence::%player%::Story::Kingdom} is not "None":
			uncolored {Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Owner} is player:
				index of event-slot is 15:
					execute player command "/removekingdom %uncolored {Commence::%player%::Story::Kingdom}%"
			index of event-slot is 12:
				execute player command "/listkingdoms"
			index of event-slot is 13:
				execute player command "/kingdom %uncolored {Commence::%player%::Story::Kingdom}%"
			index of event-slot is 14:
				execute player command "/leavekingdom %uncolored {Commence::%player%::Story::Kingdom}%"
		else:
			index of event-slot is 10, 11, 12, 13, 14, 15, 16, 19, 20, 21, 22, 23, 24, 25, 28, 29, 30, 31, 32, 33, 34, 37, 38, 39, 40, 41, 42 or 43:
				{Commence::%player%::Kingdom%index of event-slot + 9%::Name} is set:
					execute player command "/joinkingdom %{Commence::%player%::Kingdom%index of event-slot + 9%::Name}%"
		index of event-slot is 8:
			close inventory of player

command /warps [<text>] [<player>]:
	aliases: warp
	usage: &c /warps
	description: &7 Views the warps
	cooldown: 1 second
	cooldown message: {@cooldownmessage}
	trigger:
		close inventory of player
		wait 1 tick
		player's world is {@worlds}:
			arg-2 is set:
				{Commence::%player%::Admin} is true:
					{Warp::%arg-1%} is set:
						teleport arg-2 to {Warp::%arg-1%::TP}
						send ""
						send "{@commence}"
						send "&7   You sent %arg-2's display name% to <##D0944D>%uncolored {Warp::%arg-1%}%"
						stop
					else:
						send ""
						send "{@commence}"
						send "&c   %arg-1% is not a valid location"
						stop
			else if arg-1 is set:
				{Warp::%arg-1%} is set:
					teleport player to {Warp::%arg-1%::TP}
					send ""
					send "{@commence}"
					send "&7   You warped to <##D0944D>%uncolored {Warp::%arg-1%}%"
					stop
				else:
					send ""
					send "{@commence}"
					send "&c   %arg-1% is not a valid location"
					stop
			else:
				set {Commence::List} to 0
				loop {Warp::Warps::*}:
					add 1 to {Commence::List}
				loop {Warp::Warps.S::*}:
					{Commence::%player%::Admin} is true:
						add 1 to {Commence::List}
				if {Commence::List} is more than 21:
					set {_t} to 6
				else if {Commence::List} is more than 14:
					set {_t} to 5
				else if {Commence::List} is more than 7:
					set {_t} to 4
				else:
					set {_t} to 3
				{Commence::List} is less than 1:
					send "&c   Sorry, no warps are set right now!"
					stop
				set metadata tag "warps" of player to chest inventory with {_t} rows named "&8-==&k|<##D0944D>      Mythical Warps      &8&k|&8==-"
				set slot 0 of metadata tag "warps" of player to dark gray stained glass pane named "&7"
				loop 53 times:
					set slot loop-value of metadata tag "warps" of player to dark gray stained glass pane named "&7"
				set slot 4 of metadata tag "warps" of player to book named "&8-&k|<##D0944D> Browse Warp Locations &8&k|&8-" with lore "&7" and "&c Hover over warps to view info" and "&4 Click warps to teleport there"
				set {Commence::List} to 9
				loop {Warp::Warps::*}:
					add 1 to {Commence::List}
					{Commence::List} is 17:
						add 2 to {Commence::List}
					else if {Commence::List} is 26:
						add 2 to {Commence::List}
					else if {Commence::List} is 35:
						add 2 to {Commence::List}
					else if {Commence::List} is more than 43:
						stop loop
					set slot {Commence::List} of metadata tag "warps" of player to glowing paper named "&8-&k|<##D0944D> %loop-value% Warp &8&k|&8-" with lore "&7 Use: <##D0944D>/warp %loop-value%" and "&8 Or: <##D0944D>Click HERE" and "&7 Set by: <##D0944D>%{Warp::%loop-value%::Owner}%" and "&8 Location: {@mythicalrealms}"
				loop {Warp::Warps.S::*}:
					{Commence::%player%::Admin} is true:
						add 1 to {Commence::List}
					{Commence::List} is 17:
						add 2 to {Commence::List}
					else if {Commence::List} is 26:
						add 2 to {Commence::List}
					else if {Commence::List} is 35:
						add 2 to {Commence::List}
					else if {Commence::List} is more than 43:
						stop loop
					set slot {Commence::List} of metadata tag "warps" of player to glowing paper named "&8-&k|<##5BAF70> Secret <##D0944D>%loop-value% Warp &8&k|&8-" with lore "&7 Set by: <##D0944D>%{Warp::%loop-value%::Owner}%" and "&8 Location: {@mythicalrealms}"
				set slot 8 of metadata tag "warps" of player to barrier named "&4-&k|&c &lClick to Close &4&k|&4-"
				open (metadata tag "warps" of player) to player

command /setwarp <text> [<integer>]:
	aliases: createwarp
	usage: &c /setwarp [name]
	description: &7 Create a warp
	permission: commencer.admin
	cooldown: 1 second
	permission message: {@permissionmessage}
	cooldown message: {@cooldownmessage}
	trigger:
		{Commence::%player%::Admin} is true:
			{Warp::%arg-1%} is set:
				send ""
				send "{@commence}"
				send "&c   %{Warp::%arg-1%}% &calready exists"
				stop
			arg-1 is set:
				arg-2 is set:
					arg-2 is not 1:
						stop
					set {Warp::%arg-1%::TP} to player's location
					set {Warp::%arg-1%::Owner} to player's display name
					set {Warp::%arg-1%} to "<##D0944D>%arg-1%"
					add arg-1 to {Warp::Warps.S::*}
					send ""
					send "{@commence}"
					send "&7   You created a new SECRET Warp Location"
					send "&a    Created %{Warp::%arg-1%}%,"
					send "&a     at %{Warp::%arg-1%::TP}%"
					stop
				set {Warp::%arg-1%::TP} to player's location
				set {Warp::%arg-1%::Owner} to player's display name
				set {Warp::%arg-1%} to "<##D0944D>%arg-1%"
				add arg-1 to {Warp::Warps::*}
				send ""
				send "{@commence}"
				send "&7   You created a new Warp Location"
				send "&a    Created %{Warp::%arg-1%}%,"
				send "&a     at %{Warp::%arg-1%::TP}%"

command /delwarp <text>:
	usage: &c /delwarp [name]
	description: &7 Delete a warp
	permission: commencer.admin
	cooldown: 1 second
	permission message: {@permissionmessage}
	cooldown message: {@cooldownmessage}
	trigger:
		{Commence::%player%::Admin} is true:
			arg-1 is set:
				loop {Warp::Warps.S::*}:
					arg-1 is loop-value:
						send ""
						send "{@commence}"
						send "&7   You deleted a SECRET Warp Location"
						send "&c    Removed %{Warp::%arg-1%}%,"
						send "&c     at %{Warp::%arg-1%::TP}%"
						clear {Warp::%arg-1%::TP}
						clear {Warp::%arg-1%::Owner}
						remove loop-value from {Warp::Warps::*}
						remove loop-value from {Warp::Warps.S::*}
						clear {Warp::%arg-1%}
						stop loop
						stop
				loop {Warp::Warps::*}:
					loop-value contains arg-1:
						send ""
						send "{@commence}"
						send "&7   You deleted a Warp Location"
						send "&c    Removed %{Warp::%arg-1%}%,"
						send "&c     at %{Warp::%arg-1%::TP}%"
						clear {Warp::%arg-1%::TP}
						clear {Warp::%arg-1%::Owner}
						remove loop-value from {Warp::Warps::*}
						remove loop-value from {Warp::Warps.S::*}
						clear {Warp::%arg-1%}

command /homes [<text>] [<offlineplayer>]:
	aliases: /home, /sethome, /delhome, /createhome, /removehome
	usage: &c /home
	description: &7 Views Your Homes
	cooldown: 1 second
	cooldown message: {@cooldownmessage}
	trigger:
		close inventory of player
		wait 1 tick
		player's world is {@worlds}:
			command contains "sethome" or "createhome":
				arg-1 is set:
					loop {Commence::%player%::Homes::All::*}:
						uncolored loop-value contains uncolored {Commence::%player%::Homes::%arg-1%}:
							send ""
							send "{@storyhomes}"
							send "<##CB625F>   You already have a home called %arg-1%"
							stop
					set {Commence::%player%::Homes::%arg-1%::TP} to player's location
					set {Commence::%player%::Homes::%arg-1%} to "<##D0944D>%arg-1%"
					add {Commence::%player%::Homes::%arg-1%} to {Commence::%player%::Homes::All::*}
					send ""
					send "{@storyhomes}"
					send "<##00C889>   You set a new home called %{Commence::%player%::Homes::%arg-1%}%"
					send "<##00C889>    at %{Commence::%player%::Homes::%arg-1%::TP}%"
					stop
			else if command contains "delhome" or "removehome":
				arg-1 is set:
					stop if {Commence::%player%::Homes::%arg-1%} is not set
					send ""
					send "{@storyhomes}"
					send "<##CB625F>   You removed home %{Commence::%player%::Homes::%arg-1%}%"
					remove {Commence::%player%::Homes::%arg-1%} from {Commence::%player%::Homes::*} and {Commence::%player%::Homes::All::*}
					clear {Commence::%player%::Homes::%arg-1%}
					clear {Commence::%player%::Homes::%arg-1%::TP}
					stop
				else:
					loop {Commence::%player%::Homes::All::*}:
						set {_th} to loop-value
					stop if {_th} is not set
					send ""
					send "{@storyhomes}"
					send "<##CB625F>   You removed home %{Commence::%player%::Homes::%{_th}%}%"
					remove {Commence::%player%::Homes::%{_th}%} from {Commence::%player%::Homes::*} and {Commence::%player%::Homes::All::*}
					clear {Commence::%player%::Homes::%{_th}%}
					clear {Commence::%player%::Homes::%{_th}%::TP}
					stop
			arg-2 is set:
				{Commence::%player%::Inspector} is true:
					{Commence::%player%::Homes::%arg-1%} is set:
						teleport arg-2 to {Commence::%player%::Homes::%arg-1%::TP}
						send ""
						send "{@storyhomes}"
						send "&7   You sent %arg-2's display name ? arg-2% to %{Commence::%player%::Homes::%arg-1%}%"
						send "&7   %arg-2's display name ? arg-2%'s Homes:"
						send "&7    %{Commence::%arg-2%::Homes::All::*}%"
						stop
					else if {Commence::%arg-2%::Homes::%arg-1%} is set:
						teleport player to {Commence::%arg-2%::Homes::%arg-1%::TP}
						send ""
						send "{@storyhomes}"
						send "&7   You teleported to %arg-2's display name ? arg-2%&7's %{Commence::%arg-2%::Homes::%arg-1%}%"
						send "&7   %arg-2's display name ? arg-2%'s Homes:"
						send "&7    %{Commence::%arg-2%::Homes::All::*}%"
						stop
					else:
						send ""
						send "{@storyhomes}"
						send "&c   %arg-1% is not a valid location"
						stop
			else if arg-1 is set:
				{Commence::%player%::Homes::%arg-1%} is set:
					teleport player to {Commence::%player%::Homes::%arg-1%::TP}
					send ""
					send "{@storyhomes}"
					send "&7   You teleported to home %{Commence::%player%::Homes::%arg-1%}%"
					stop
				else:
					send ""
					send "{@storyhomes}"
					send "&c   %arg-1% is not a valid location"
					stop
			else:
				set {Commence::List} to 0
				loop {Commence::%player%::Homes::All::*}:
					add 1 to {Commence::List}
					set {_th} to loop-value
				if {Commence::List} is more than 21:
					set {_t} to 6
				else if {Commence::List} is more than 14:
					set {_t} to 5
				else if {Commence::List} is more than 7:
					set {_t} to 4
				else:
					set {_t} to 3
				send ""
				send "{@storyhomes}"
				send "<##00C889>   Below is a list of all your homes!"
				send "&7   Type /home [name] or click the compass to teleport!"
				send "<##CB625F>    %uncolored {Commence::%player%::Homes::All::*} ? {Commence::List}%"
				set metadata tag "homes" of player to chest inventory with 3 rows named "&8-==&k|<##D0944D>      Mythical Homes      &8&k|&8==-"
				set slot 0 of metadata tag "homes" of player to dark gray stained glass pane named "&7"
				loop 53 times:
					set slot loop-value of metadata tag "homes" of player to dark gray stained glass pane named "&7"
				set slot 4 of metadata tag "homes" of player to book named "&8-&k|<##D0944D> Browse Your Homes &8&k|&8-" with lore "&7" and "&c Hover over homes to view info" and "&4 Click homes to teleport there"
				{Commence::List} is less than 1:
					set slot 13 of metadata tag "homes" of player to glowing compass named "&8-&k|<##D0944D> Create a New Home &8&k|&8-" with lore "&7 Set by: <##D0944D>Clicking HERE"
					set slot 8 of metadata tag "homes" of player to barrier named "&4-&k|&c &lClick to Close &4&k|&4-"
					open (metadata tag "homes" of player) to player
					stop
				set slot 12 of metadata tag "homes" of player to glowing paper named "&8-&k|<##D0944D> Redefine %{_th}%'s Location &8&k|&8-" with lore "&8 Reset by: <##D0944D>Clicking HERE"
				set slot 13 of metadata tag "homes" of player to glowing compass named "&8-&k|<##3988A3> Teleport to %{_th}% &8&k|&8-" with lore "&7 Use: <##D0944D>/home %{_th}%" and "&8 Or: <##D0944D>Click HERE" and "&7 Set by: <##D0944D>%{Commence::%player%::DisplayName}%" and "&8 Location: {@mythicalrealms}"
				set slot 14 of metadata tag "homes" of player to glowing paper named "&8-&k|<##CB625F> Remove Home %{_th}% &8&k|&8-" with lore "&7 Set by: <##D0944D>%{Commence::%player%::DisplayName}%" and "&8 Location: {@mythicalrealms}"
				set slot 8 of metadata tag "homes" of player to barrier named "&4-&k|&c &lClick to Close &4&k|&4-"
				open (metadata tag "homes" of player) to player

command /kingdoms [<text>] [<player>]:
	aliases: /kingdom, /createkingdom, /removekingdom, /joinkingdom, /leavekingdom, /lk, listkingdoms
	usage: &c /kingdom
	description: &7 Views Server Kingdoms
	cooldown: 1 second
	cooldown message: {@cooldownmessage}
	trigger:
		close inventory of player
		wait 1 tick
		player's world is {@worlds}:
			{Commence::%player%::Admin} is true:
				command contains "add" or "create":
					{Commence::%player%::Story::Kingdom} is not "None":
						send ""
						send "{@storykingdoms}"
						send "&c   You're Already a Part of The %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%}%&c Kingdom"
						stop
					arg-1 is set:
						block under player is air or water or lava:
							send ""
							send "{@storykingdoms}"
							send "&c   Try a Safer Location Before Trying Again"
							stop
						set {Commence::Kingdoms::%arg-1%::TP} to player's location
						set {Commence::Kingdoms::%arg-1%::Type} to "<##D0944D>Hamlet"
						set {Commence::Kingdoms::%arg-1%::Funds} to 100
						set {Commence::Kingdoms::%arg-1%::Owner} to player's display name
						set {Commence::Kingdoms::%arg-1%} to arg-1 in proper case
						set {Commence::Kingdoms::%arg-1%} to "<##8ACCBE>%{Commence::Kingdoms::%arg-1%}%"
						set {Commence::%player%::Story::Kingdom} to {Commence::Kingdoms::%arg-1%}
						add {Commence::Kingdoms::%arg-1%} to {Commence::Kingdoms::List::*}
						add player to {Commence::Kingdoms::%arg-1%::Members::*}
						send ""
						send "{@storykingdoms}"
						send "<##00C889>   You Just Created a New Kingdom"
						send "<##00C889>    Called The %{Commence::Kingdoms::%arg-1%}%<##00C889> %{Commence::Kingdoms::%arg-1%::Type}%"
						send "<##00C889>     with Owner: %{Commence::Kingdoms::%arg-1%::Owner}%"
						send "<##00C889>     and Type: %{Commence::Kingdoms::%arg-1%::Type}%"
						send "<##00C889>     at %{Commence::Kingdoms::%arg-1%::TP}%"
						send "<##00C889> "
						send "<##00C889>  Manage it in <##D0944D>/kingdoms"
						stop
				else if command contains "del" or "remove":
					uncolored {Commence::Kingdoms::%arg-1%::Owner} is player:
						{Commence::Kingdoms::%arg-1%} is not set:
							stop
						set {Commence::%player%::Story::Kingdom} to "None"
						send ""
						send "{@storykingdoms}"
						send "<##CB625F>   You removed the %{Commence::Kingdoms::%arg-1%}%<##CB625F> %{Commence::Kingdoms::%arg-1%::Type}%"
						remove {Commence::Kingdoms::%arg-1%} from {Commence::Kingdoms::List::*}
						clear {Commence::Kingdoms::%arg-1%::*}
						stop
					else:
						arg-1 is set:
							send ""
							send "{@storykingdoms}"
							send "&c   %arg-1% is not a valid Kingdom"
							stop
			command contains "list":
				set {Commence::List} to 0
				loop {Commence::Kingdoms::List::*}:
					add 1 to {Commence::List}
				if {Commence::List} is more than 21:
					set {_t} to 6
				else if {Commence::List} is more than 14:
					set {_t} to 5
				else if {Commence::List} is more than 7:
					set {_t} to 4
				else:
					set {_t} to 3
				set metadata tag "kingdoms" of player to chest inventory with {_t} rows named "&8-==&k|<##D0944D>    Mythical Kingdoms    &8&k|&8==-"
				set slot 0 of metadata tag "kingdoms" of player to dark gray stained glass pane named "&7"
				loop 53 times:
					set slot loop-value of metadata tag "kingdoms" of player to dark gray stained glass pane named "&7"
				set slot 4 of metadata tag "kingdoms" of player to book named "&8-&k|<##D0944D> Browse Kingdoms &8&k|&8-" with lore "&7" and "&c Hover over kingdoms to view info" and "&4 Click kingdoms to teleport there"
				set {Commence::List} to 9
				loop {Commence::Kingdoms::List::*}:
					add 1 to {Commence::List}
					{Commence::List} is 17:
						add 2 to {Commence::List}
					else if {Commence::List} is 26:
						add 2 to {Commence::List}
					else if {Commence::List} is 35:
						add 2 to {Commence::List}
					{Commence::List} is less than 10:
						send ""
						send "{@storykingdoms}"
						send "&c   There are no Kingdom's available to Join right now"
						send "&c   Why don't you make one? Type <##D0944D>/createkingdom [name]"
						stop
					set slot {Commence::List} of metadata tag "kingdoms" of player to glowing paper named "&8-&k|<##D0944D> The %loop-value%<##D0944D> %{Commence::Kingdoms::%uncolored loop-value%::Type}%&8&k|&8-" with lore "&7" and "&7 Owner: <##D0944D>%{Commence::Kingdoms::%uncolored loop-value%::Owner}%" and "&8 Type: <##D0944D>%{Commence::Kingdoms::%uncolored loop-value%::Type}%" and "&7 Location: {@mythicalrealms}" and "&7"
				set slot 8 of metadata tag "kingdoms" of player to barrier named "&4-&k|&c &lClick to Close &4&k|&4-"
				open (metadata tag "kingdoms" of player) to player
				stop
			command contains "add" or "create":
				{Commence::%player%::Story::Kingdom} is not "None":
					send ""
					send "{@storykingdoms}"
					send "&c   You're Already a Part of The %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%}%&c %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}%"
					stop
				arg-1 is set:
					block under player is air or water or lava:
						send ""
						send "{@storykingdoms}"
						send "&c   Try a Safer Location Before Trying Again"
						stop
					set {Commence::Kingdoms::%arg-1%::TP} to player's location
					set {Commence::Kingdoms::%arg-1%::Type} to "<##D0944D>Hamlet"
					set {Commence::Kingdoms::%arg-1%::Funds} to 100
					set {Commence::Kingdoms::%arg-1%::Owner} to player's display name
					set {Commence::Kingdoms::%arg-1%} to arg-1 in proper case
					set {Commence::Kingdoms::%arg-1%} to "<##8ACCBE>%{Commence::Kingdoms::%arg-1%}%"
					set {Commence::%player%::Story::Kingdom} to {Commence::Kingdoms::%arg-1%}
					add {Commence::Kingdoms::%arg-1%} to {Commence::Kingdoms::List::*}
					add player to {Commence::Kingdoms::%arg-1%::Members::*}
					send ""
					send "{@storykingdoms}"
					send "<##00C889>   You Just Created a New Kingdom"
					send "<##00C889>    Called The %{Commence::Kingdoms::%arg-1%}%<##00C889> %{Commence::Kingdoms::%arg-1%::Type}%"
					send "<##00C889>     with Owner: %{Commence::Kingdoms::%arg-1%::Owner}%"
					send "<##00C889>     and Type: %{Commence::Kingdoms::%arg-1%::Type}%"
					send "<##00C889>     at %{Commence::Kingdoms::%arg-1%::TP}%"
					send "<##00C889> "
					send "<##00C889>  Manage it in <##D0944D>/kingdoms"
					stop
			command contains "join":
				{Commence::%player%::Story::Kingdom} is not "None":
					send ""
					send "{@storykingdoms}"
					send "&c   You're Already a Part of The %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%}%&c %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}%"
					stop
				{Commence::Kingdoms::%arg-1%} is set:
					set {Commence::%player%::Story::Kingdom} to {Commence::Kingdoms::%arg-1%}
					add player to {Commence::Kingdoms::%arg-1%::Members::*}
					send ""
					send "{@storykingdoms}"
					send "<##00C889>   You Just Joined a Kingdom"
					send "<##00C889>   Called The %{Commence::Kingdoms::%arg-1%}%<##00C889> %{Commence::Kingdoms::%arg-1%::Type}%"
					send "<##00C889>    with Owner: %{Commence::Kingdoms::%arg-1%::Owner}%"
					send "<##00C889>    and Type: %{Commence::Kingdoms::%arg-1%::Type}%"
					send "<##00C889>    in %{Commence::Kingdoms::%arg-1%::TP}'s world%"
					send "<##00C889> "
					send "<##CB625F>   Manage it in <##D0944D>/kingdoms"
					stop
				else:
					send ""
					send "{@storykingdoms}"
					send "&c   %arg-1% is not a valid Kingdom"
					stop
			command contains "leave":
				uncolored {Commence::Kingdoms::%arg-1 ? uncolored {Commence::%player%::Story::Kingdom}%::Owner} is player:
					send ""
					send "{@storykingdoms}"
					send "&c   You're the Owner and Can't Leave The %{Commence::Kingdoms::%arg-1 ? uncolored {Commence::%player%::Story::Kingdom}%}%&c %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}%"
					stop
				{Commence::%player%::Story::Kingdom} is not "None":
					send ""
					send "{@storykingdoms}"
					send "<##00C889>   You Just Left The %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%}%<##00C889> %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}%"
					set {Commence::%player%::Story::Kingdom} to "None"
					remove player from {Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Members::*}
					stop
				else:
					arg-1 is not set:
						stop
					send ""
					send "{@storykingdoms}"
					send "&c   You Aren't a Part of The %arg-1%&c %{Commence::Kingdoms::%arg-1%::Type}%"
					stop
			arg-2 is set:
				{Commence::Kingdoms::%arg-1%} is set:
					teleport arg-2 to {Commence::Kingdoms::%arg-1%::TP}
					send "" to arg-2
					send "{@storykingdoms}" to arg-2
					send "&7   %player's display name% sent you to the %{Commence::Kingdoms::%arg-1%}%&7 %{Commence::Kingdoms::%arg-1%::Type}%" to arg-2
					stop
					send ""
					send "{@storykingdoms}"
					send "&7   You sent %arg-2's display name% to the %{Commence::Kingdoms::%arg-1%}%&7 %{Commence::Kingdoms::%arg-1%::Type}%"
					stop
				else:
					send ""
					send "{@storykingdoms}"
					send "&c   %arg-1% is not a valid %{Commence::Kingdoms::%arg-1%::Type}%"
					stop
			else if arg-1 is set:
				{Commence::Kingdoms::%arg-1%} is set:
					teleport player to {Commence::Kingdoms::%arg-1%::TP}
					send ""
					send "{@storykingdoms}"
					send "&7   You teleported to the %{Commence::Kingdoms::%arg-1%}%&7 %{Commence::Kingdoms::%arg-1%::Type}%"
					stop
				else:
					send ""
					send "{@storykingdoms}"
					send "&c   %arg-1% is not a valid %{Commence::Kingdoms::%arg-1%::Type}%"
					stop
			else:
				set {Commence::List} to 0
				loop {Commence::Kingdoms::List::*}:
					add 1 to {Commence::List}
				if {Commence::List} is more than 21:
					set {_t} to 6
				else if {Commence::List} is more than 14:
					set {_t} to 5
				else if {Commence::List} is more than 7:
					set {_t} to 4
				else:
					set {_t} to 3
				set metadata tag "kingdoms" of player to chest inventory with {_t} rows named "&8-==&k|<##D0944D>    Mythical Kingdoms    &8&k|&8==-"
				set slot 0 of metadata tag "kingdoms" of player to dark gray stained glass pane named "&7"
				loop {Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Members::*}:
					set {_tm} to loop-index parsed as integer
				{_tm} is more than 7:
					wait 1 tick
				else:
					set {_tm} to "%{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Members::*}%"
				loop 53 times:
					set slot loop-value of metadata tag "kingdoms" of player to dark gray stained glass pane named "&7"
				set slot 4 of metadata tag "kingdoms" of player to book named "&8-&k|<##D0944D> Browse Kingdoms &8&k|&8-" with lore "&7" and "&c Hover over kingdoms to view info" and "&4 Click kingdoms to teleport there"
				{Commence::%player%::Story::Kingdom} is not "None":
					uncolored {Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Owner} is player:
						set slot 11 of metadata tag "kingdoms" of player to glowing map named "&8-&k|<##D0944D> The %{Commence::%player%::Story::Kingdom}%<##D0944D> %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}%'s Members &8&k|&8-" with lore "&7 Owner: <##D0944D>%{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Owner}%" and "&8 Members: <##D0944D>%{_tm}%" and "&7 Location: {@mythicalrealms}" and "&7"
						set slot 15 of metadata tag "kingdoms" of player to glowing map named "&8-&k|<##CB625F> Delete the %{Commence::%player%::Story::Kingdom}%<##CB625F> %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}% &8&k|&8-" with lore "&7" and "&7 Owner: <##D0944D>%{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Owner}%" and "&8 Type: <##D0944D>%{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}%" and "&7 Location: {@mythicalrealms}" and "&7" and "<##00608D> Delete by Clicking <##CB625F>HERE" and "&7"
					set slot 12 of metadata tag "kingdoms" of player to glowing paper named "&8-&k|<##D0944D> View All Kingdoms &8&k|&8-" with lore "&7" and "&7 View: <##D0944D>by Clicking HERE" and "&7  or: <##D0944D>type /listkingdom" and "&7" and "<##00608D> View List by Clicking <##00C889>HERE" and "&7"
					set slot 13 of metadata tag "kingdoms" of player to glowing cobblestone wall named "&8-&k|<##D0944D> The %{Commence::%player%::Story::Kingdom}%<##D0944D> %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}% &8&k|&8-" with lore "&7" and "&7 Owner: <##D0944D>%{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Owner}%" and "&8 Members: <##D0944D>%{_tm}%" and "&7 Money: <##D0944D>$%{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Funds}%" and "&8 Type: <##D0944D>%{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}%" and "&7 Location: {@mythicalrealms}" and "&7" and "<##00608D> Teleport by Clicking <##CB625F>HERE" and "&7"
					set slot 14 of metadata tag "kingdoms" of player to glowing paper named "&8-&k|<##CB625F> Leave the %{Commence::%player%::Story::Kingdom}%<##CB625F> %{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}% &8&k|&8-" with lore "&7" and "&7 Owner: <##D0944D>%{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Owner}%" and "&8 Type: <##D0944D>%{Commence::Kingdoms::%uncolored {Commence::%player%::Story::Kingdom}%::Type}%" and "&7 Location: {@mythicalrealms}" and "&7" and "<##00608D> Leave by Clicking <##CB625F>HERE" and "&7"
					set slot 8 of metadata tag "kingdoms" of player to barrier named "&4-&k|&c &lClick to Close &4&k|&4-"
					open (metadata tag "kingdoms" of player) to player
					stop
				else:
					set {Commence::List} to 9
					loop {Commence::Kingdoms::List::*}:
						add 1 to {Commence::List}
						{Commence::List} is 17:
							add 2 to {Commence::List}
						else if {Commence::List} is 26:
							add 2 to {Commence::List}
						else if {Commence::List} is 35:
							add 2 to {Commence::List}
						{Commence::List} is less than 10:
							send ""
							send "{@storykingdoms}"
							send "&c   There are no Kingdom's available to Join right now"
							send "&c   Why don't you make one? Type <##D0944D>/createkingdom [name]"
							stop
						set {Commence::%player%::Kingdom%loop-index%::Name} to loop-value
						set slot {Commence::List} of metadata tag "kingdoms" of player to glowing paper named "&8-&k|<##D0944D> The %loop-value%<##D0944D> %{Commence::Kingdoms::%uncolored loop-value%::Type}%&8&k|&8-" with lore "&7" and "&7 Owner: <##D0944D>%{Commence::Kingdoms::%uncolored loop-value%::Owner}%" and "&8 Type: <##D0944D>%{Commence::Kingdoms::%uncolored loop-value%::Type}%" and "&7 Location: {@mythicalrealms}" and "&7" and "<##00608D> Join by Clicking <##00C889>HERE" and "&7"
					set slot 8 of metadata tag "kingdoms" of player to barrier named "&4-&k|&c &lClick to Close &4&k|&4-"
					open (metadata tag "kingdoms" of player) to player

#-=-=-=-=-=-=-=-=-=-
	#REPORTER

command /help:
	aliases: /helper, /h, /?, /about, /bukkit:help, /bukkit:?, /bukkit:about
	cooldown: 2 seconds
	trigger:
		send ""
		send "{@helper}"
		send "&7    Hover over the options to view more information"
		send "&7    Click the options to view more detailed information"
		send "&7    For even more information, type &a/commands"
		send "&7    If you have something to say, type &d/report &c[text]"
		send "&7    For further assistance, contact a &4/staff&7 member"
		execute player command "/info"

command /viewreports [<text>]:
	aliases: /vr
	cooldown: 2 seconds
	permission: Commencer.Admin
	trigger:
		arg-1 is set:
			send ""
			send "{@helper}"
			send "&7  Here is a list of all reports submitted."
			loop {Commence::Report::*}:
				send "&7   %loop-value%"
			send "&7  ...and now they're cleared."
			clear {Commence::Report::*}
		else:
			send ""
			send "{@helper}"
			send "&7  Here is a list of all reports submitted."
			loop {Commence::Report::*}:
				send "&7   %loop-value%"

command /report [<text>]:
	aliases: /suggest, /re, /amsg, /feedback
	cooldown: 2 seconds
	trigger:
		if arg-1 is set:
			set {_te} to player's display name
			set {_tem} to arg-1
			add " %{_te}%&7: %{_tem}%" to {Commence::Report::*}
			send ""
			send "{@helper}"
			send "&7  Thank you for submitting to &9Commence&8SMP&c staff"
			wait 10 ticks
			send "&7  We will review and take everything into consideration!"
			send "&d  Your Message: %arg-1%"
			wait 18 ticks
			send "&7  You should hear from us soon. Thank you again!"
		else:
			send ""
			send "{@helper}"
			send "&7  Thank you for using the &dReporter"
			wait 12 ticks
			send "&7  To report something, type the following:"
			wait 19 ticks
			send "&d  /report &c[ your comment, question, suggestion, concern here ]"

#-=-=-=-=-=-=-=-=-=-
	#BASIC COMMANDS

command /website:
	aliases: ws
	usage: &c/ website
	description: &7 Views the website
	cooldown: 5 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		send ""
		send "{@commence}"
		send "&7   We have a Website! Click below:"
		send "&8    [ <link:https://www.CommenceSMP.com><tooltip:&8Click to Open The &aWebsite &8Link>Click Here to Open the &aWebsite &8]"

command /cheer <text>:
	aliases: announc, prais
	usage: &c/ welcomeback [ PLAYER ]
	description: &7 Welcomes a player back to the server
	cooldown: 1 minute
	cooldown message: {@cooldownmessage}
	trigger:
		broadcast "  &9%{Commence::%player%::DisplayName}%&7 is %command%ing for &9%arg-1%&7!"

command /wb <player>:
	aliases: welcomeback, welback, welb
	usage: &c/ welcomeback [ PLAYER ]
	description: &7 Welcomes a player back to the server
	cooldown: 10 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		broadcast "  &9%{Commence::%player%::DisplayName}%&7: Welcome back, %arg-1's display name%&7!:)"

command /welcome <player>:
	aliases: wel
	usage: &c/ welcome [ PLAYER ]
	description: &7 Welcomes a player to the server
	cooldown: 10 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		broadcast "  &9%{Commence::%player%::DisplayName}%&7: Welcome, %arg-1's display name%&7!:)"

command /bye <player>:
	usage: &c/ bye [ PLAYER ]
	description: &7 Welcomes a player to the server
	cooldown: 10 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		broadcast "  &9%{Commence::%player%::DisplayName}%&7: See ya next time, %arg-1's display name%&7!:)" to player's world

command /discord:
	usage: &c /discord
	description: &7 Views the discord
	cooldown: 10 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		send ""
		send "{@commence}"
		send "&7   We have a &5Discord &7server! Click below:"
		send "&8    [ <link:https://discord.gg/Qq43r2mZzs><tooltip:&8Click to Open The &5Discord &8Link>Click Here to Open &5Discord &8]"

command /donate:
	usage: &c /donate
	description: &7 Views the website
	cooldown: 5 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		send ""
		send "{@commence}"
		send "&7   Currently, donations are done directly to us"
		send "&7    Our /website will have donation info soon!"

#-=-=-=-=-=-=-=-=-=-
	#NEW GUI

on inventory click:
	if event-inventory is (metadata tag "adminpanel" of player):
		cancel event
		index of event-slot is 10:
			{Survival::Enabled} is true:
				set {Survival::Enabled} to false
				send "&c   You disabled &fOGSurvival"
			else:
				set {Survival::Enabled} to true
				send "&a   You enabled &fOGSurvival"
		index of event-slot is 19:
			{Lifer::Enabled} is true:
				set {Lifer::Enabled} to false
				send "&c   You disabled &aLifer"
			else:
				set {Lifer::Enabled} to true
				send "&a   You enabled Lifer"
		index of event-slot is 28:
			{CS::Enabled} is true:
				set {CS::Enabled} to false
				send "&c   You disabled {@mythicalrealms}"
			else:
				set {CS::Enabled} to true
				send "&a   You enabled {@mythicalrealms}"
		index of event-slot is 37:
			{PVP::Enabled} is true:
				set {PVP::Enabled} to false
				send "&c   You disabled KitPvP"
			else:
				set {PVP::Enabled} to true
				send "&a   You enabled &cKitPvP"
		index of event-slot is 8:
			close inventory of player
		else if index of event-slot is 4:
			execute player command "/c"
		else:
			execute player command "/ap"
	if event-inventory is (metadata tag "commence" of player):
		cancel event
		index of event-slot is 8:
			close inventory of player
		index of event-slot is 4:
			close inventory of player
			execute player command "/staff"
		index of event-slot is 11:
			{CS::Enabled} is true:
				close inventory of player
				execute player command "/cs"
			else:
				close inventory of player
				execute player command "/l"
		index of event-slot is 13:
			{Survival::Enabled} is true:
				close inventory of player
				execute player command "/survival"
		index of event-slot is 15:
			close inventory of player
			execute player command "/report"
		index of event-slot is 10:
			close inventory of player
			execute player command "/help"
#		index of event-slot is 14:
#			{Lifer::Enabled} is true:
#				close inventory of player
#				execute player command "/lifer"
	if event-inventory is (metadata tag "plugins" of player):
		cancel event
		index of event-slot is 8:
			close inventory of player
		index of event-slot is 4:
			close inventory of player
			execute player command "/info"
	if event-inventory is (metadata tag "rules" of player):
		cancel event
		index of event-slot is 8:
			close inventory of player
		index of event-slot is 4:
			close inventory of player
			execute player command "/info"
	if event-inventory is (metadata tag "ranks" of player):
		cancel event
		index of event-slot is 8:
			close inventory of player
		index of event-slot is 4:
			close inventory of player
			execute player command "/info"
	if event-inventory is (metadata tag "online" of player):
		cancel event
		index of event-slot is 8:
			close inventory of player
		index of event-slot is 4:
			close inventory of player
			execute player command "/staff"
	if event-inventory is (metadata tag "staff" of player):
		cancel event
		index of event-slot is 8:
			close inventory of player
		index of event-slot is 4:
			close inventory of player
			execute player command "/online"
	if event-inventory is (metadata tag "sidebar" of player):
		cancel event
		index of event-slot is 8:
			close inventory of player
		index of event-slot is 10:
			execute player command "/sidebar survival"
		index of event-slot is 12:
			execute player command "/sidebar story"
		index of event-slot is 14:
			execute player command "/sidebar spawnhub"
		index of event-slot is 16:
			execute player command "/sidebar lifer"
	if event-inventory is (metadata tag "infos" of player):
		cancel event
		index of event-slot is 8:
			close inventory of player
		index of event-slot is 4:
			close inventory of player
			execute player command "/online"
		index of event-slot is 19:
			close inventory of player
			execute player command "/ranks"
		index of event-slot is 20:
			close inventory of player
			send "&c   Sorry, this feature is coming soon"
			execute player command "/guide"
		index of event-slot is 21:
			close inventory of player
			execute player command "/plugins"
		index of event-slot is 22:
			close inventory of player
			execute player command "/c"
		index of event-slot is 23:
			close inventory of player
			send "&c   Sorry, this feature is coming soon"
			execute player command "/mechanics"
		index of event-slot is 24:
			close inventory of player
			execute player command "/commands"
		index of event-slot is 25:
			close inventory of player
			send "&c   Sorry, this feature is coming soon"
			execute player command "/plans"
		index of event-slot is 28:
			close inventory of player
			execute player command "/shop"
		index of event-slot is 29:
			close inventory of player
			execute player command "/rules"
		index of event-slot is 30:
			close inventory of player
			execute player command "/website"
		index of event-slot is 31:
			close inventory of player
			execute player command "/discord"
		index of event-slot is 32:
			close inventory of player
			execute player command "/donate"
		index of event-slot is 33:
			close inventory of player
			execute player command "/helper"
		index of event-slot is 34:
			close inventory of player
			execute player command "/online"

#-=-=-=-=-=-=-=-=-=-
	#NEW GUI COMMANDS

command /info [<text>]:
	aliases: /ifo, /news, /eabout, /cinfo, /in, /help, /bukkit:help, /essentials:help, /minecraft:help
	usage: &c /info [ RANKS | DONATE | WEBSITE | LIFER | COMMANDS | PLUGINS ]
	description: &7 Views basic information about the server
	cooldown: 5 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		close inventory of player
		wait 1 tick
		argument is not set:
			set metadata tag "infos" of player to chest inventory with 4 rows named "&8-==&k|&9   {@commencesmp}&8 Help   &8&k|&8==-"
			set slot 0 of metadata tag "infos" of player to dark gray stained glass pane named "&7"
			loop 44 times:
				set slot loop-value of metadata tag "infos" of player to dark gray stained glass pane named "&7"
