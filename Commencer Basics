command /c [<world>]:
	aliases: commencer, commence, com
	usage: &c /commence
	description: &7 Server Navigator
	cooldown: 1 second
	cooldown message: {@cooldownmessage}
	trigger:
		close inventory of player
		wait 1 tick
		player's world is not "SpawnHub" or "Survival":
			send "{@worldpermissionmessage}"
			stop
		set metadata tag "commence" of player to chest inventory with 3 rows named "&8-==&k|&9   <##3988A4>C<##3C91AD>o<##4099B7>m<##43A2C1>m<##49AFD1>e<##4EBBE0>n<##52C4EA>c<##55CDF4>e<##545B5E>Navigator   &8&k|&8==-"
