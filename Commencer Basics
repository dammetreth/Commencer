command /staff:
	aliases: admins
	usage: &c /staff
	description: &7 Views the staff on the server
	cooldown: 5 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		close inventory of player
		wait 1 tick
		clear {Commence::Admins::*}
		loop all players:
			if {Commence::%loop-player%::Admin} is true:
				add loop-player to {Commence::Admins::*}
		if {Commence::Admins::*} is not set:
			send ""
			send "{@commence}"
			send "&c   No Staff are Online Currently"
			send "&c   Type /report or &a/info"
		else:
			set metadata tag "staff" of player to chest inventory with 3 rows named "&8-==&k|&9  {@commencesmp}&c Staff  &8&k|&8==-"
			set slot 0 of metadata tag "staff" of player to dark gray stained glass pane named "&7"
			loop 44 times:
				set slot loop-value of metadata tag "staff" of player to dark gray stained glass pane named "&7"
			set slot 4 of metadata tag "staff" of player to skull of player named "&8-&k|<##3988A4> Welcome to {@commencesmp} &8&k|&8-" with lore "&7Hey there %player's display name%," and "&7 welcome to the community!" and "&8Below is a list of all online staff"
			set {Commence::List} to 9
			loop {Commence::Admins::*}:
				add 1 to {Commence::List}
				{Commence::List} is 17:
					add 2 to {Commence::List}
				else if {Commence::List} is 26:
					add 2 to {Commence::List}
				set slot {Commence::List} of metadata tag "staff" of player to skull of loop-value named "%{Commence::%loop-value%::Rank}%%loop-value's display name%"
				stop loop
			loop {Commence::Inspectors::*}:
				add 1 to {Commence::List}
				set slot {Commence::List} of metadata tag "staff" of player to skull of loop-value named "%{Commence::%loop-value%::Rank}%%loop-value's display name%" if {Commence::%loop-value%::Rank::Value} is 4
				stop loop
			set slot 8 of metadata tag "staff" of player to structure void named "&4-&k|&c &lClick to Close &4&k|&4-"
			open (metadata tag "staff" of player) to player

command /online:
	aliases: list, who, playerlist, plist
	usage: &c /list
	description: &7 Views all online players
	cooldown: 5 seconds
	cooldown message: {@cooldownmessage}
	trigger:
		close inventory of player
		wait 1 tick
		set metadata tag "online" of player to chest inventory with 4 rows named "&8-==&k|&9 {@commencesmp}&8 Players &8&k|&8==-"
		set slot 0 of metadata tag "online" of player to dark gray stained glass pane named "&7"
		loop 44 times:
			set slot loop-value of metadata tag "online" of player to dark gray stained glass pane named "&7"
		set slot 4 of metadata tag "online" of player to skull of player named "&8-&k|<##3988A4> Welcome to {@commencesmp} &8&k|&8-" with lore "&7Hey there %player's display name%," and "&7 welcome to the community!" and "&8If you need any help, use &a/info&8 or &c/staff"
		set {Commence::List} to 9
		loop all players:
			add 1 to {Commence::List}
			{Commence::List} is 17:
				add 2 to {Commence::List}
			else if {Commence::List} is 26:
				add 2 to {Commence::List}
			else if {Commence::List} is 35:
				add 2 to {Commence::List}
			set slot {Commence::List} of metadata tag "online" of player to skull of loop-value named "%{Commence::%loop-value%::Rank}%%loop-value's display name%"
		set slot 8 of metadata tag "online" of player to structure void named "&4-&k|&c &lClick to Close &4&k|&4-"
		open (metadata tag "online" of player) to player
			
command /sidebar [<text>]:
	aliases: /sb
	usage: &c /sidebar
	description: &7 Opens the Sidebar GUI
	cooldown: 1 second
	cooldown message: {@cooldownmessage}
	trigger:
		toggle player's scoreboard to off
		close inventory of player
		wait 1 tick
		arg-1 is "survival":
			{Commence::%player%::SurvivalBar} is true:
				set {Commence::%player%::SurvivalBar} to false
			else:
				toggle player's scoreboard to on
				set {Commence::%player%::SurvivalBar} to true
		else if arg-1 is "pvp":
			set {Commence::%player%::PVPBar} to true if {Commence::%player%::PVPBar} is false
			set {Commence::%player%::PVPBar} to false if {Commence::%player%::PVPBar} is true
		else if arg-1 is "lifer":
			{Commence::%player%::LiferBar} is true:
				set {Commence::%player%::LiferBar} to false
			else:
				toggle player's scoreboard to on
				set {Commence::%player%::LiferBar} to true
		else if arg-1 is "story":
			{Commence::%player%::StoryBar} is true:
				set {Commence::%player%::StoryBar} to false
			else:
				toggle player's scoreboard to on
				set {Commence::%player%::StoryBar} to true
		else if arg-1 is "spawnhub":
			{Commence::%player%::SideBar} is true:
				set {Commence::%player%::SideBar} to false
			else:
				toggle player's scoreboard to on
				set {Commence::%player%::SideBar} to true
		set metadata tag "sidebar" of player to chest inventory with 3 rows named "&8-==&k|&9 {@commencesmp}&8 Sidebar &8&k|&8==-"
		set slot 0 of metadata tag "sidebar" of player to dark gray stained glass pane named "&7"
		loop 26 times:
			set slot loop-value of metadata tag "sidebar" of player to dark gray stained glass pane named "&7"
		set slot 4 of metadata tag "sidebar" of player to book named "&8-&k|&9 Toggle Gamemode Sidebars &8&k|&8-" with lore "&7 Here you can toggle the" and "&7  sidebars for each world" and "&c NOTE it can take up to " and "&c  5 seconds to toggle" and "&7" and "&9 Click the items below to toggle"
		{Commence::%player%::SurvivalBar} is false:
			set slot 10 of metadata tag "sidebar" of player to glowing dirt named "&8-&k|&f OGSurvival Sidebar &8&k|&8-" with lore "&c Sidebar is DISABLED in &fOGSurvival" and "&7 " and "&9  Click to &aenable&9 the sidebar"
		else if {Commence::%player%::SurvivalBar} is not false:
			set slot 10 of metadata tag "sidebar" of player to glowing dirt named "&8-&k|&f OGSurvival Sidebar &8&k|&8-" with lore "&a Sidebar is ENABLED in &fOGSurvival" and "&7 " and "&9  Click to &cdisable&9 the sidebar"
		{Commence::%player%::StoryBar} is false:
			set slot 12 of metadata tag "sidebar" of player to glowing book named "&8-&k|&c {@mythicalrealms} Sidebar &8&k|&8-" with lore "&c Sidebar is DISABLED in {@mythicalrealms}" and "&7 " and "&9  Click to &aenable&9 the sidebar" with item flags hide_additional_tooltip, hide_armor_trim, hide_attributes, hide_destroys, hide_dye,hide_enchants, hide_placed_on,hide_stored_enchants, and hide_unbreakable
		else if {Commence::%player%::StoryBar} is not false:
			set slot 12 of metadata tag "sidebar" of player to glowing book named "&8-&k|&c {@mythicalrealms} Sidebar &8&k|&8-" with lore "&a Sidebar is ENABLED in {@mythicalrealms}" and "&7 " and "&8-&k|&e Icon Key &8&k|&8-" and "&7  <##00C889>🍖: Hunger (Out of 10)" and "&7  <##00C889>🪣: Thirst (Out of 10)" and "&7  <##00C889>⌚: Sleep (Out of 10)" and "&7  <##855EC9>🧪: Mana" and "&7  <##00608D>⛨: RPG Level (Level XP/Next Level XP)" and "&7  <##CB625F>🗡: Attack (Default is 1)" and "&7  <##CB625F>⚔: Defense (Default is 0)" and "&7  <##CB625F>☠: Deaths (All Game-Modes)" and "&7  <##D0944D>✳: Story Score (/mrm scoreshop)" and "&7  <##D0944D>❇: Story Position (/positionpoints)" and "&7  <##D0944D>₵: Balance (Money)" and "&7 " and "&9  Click to &cdisable&9 the sidebar" with item flags hide_additional_tooltip, hide_armor_trim, hide_attributes, hide_destroys, hide_dye,hide_enchants, hide_placed_on,hide_stored_enchants, and hide_unbreakable
		{Commence::%player%::SideBar} is false:
			set slot 14 of metadata tag "sidebar" of player to glowing compass named "&8-&k|&9 SpawnHub Sidebar &8&k|&8-" with lore "&c Sidebar is DISABLED in &9SpawnHub" and "&7 " and "&9  Click to &aenable&9 the sidebar"
		else if {Commence::%player%::SideBar} is not false:
			set slot 14 of metadata tag "sidebar" of player to glowing compass named "&8-&k|&9 SpawnHub Sidebar &8&k|&8-" with lore "&a Sidebar is ENABLED in &9SpawnHub" and "&7 " and "&9  Click to &cdisable&9 the sidebar"
		{Commence::%player%::LiferBar} is false:
			set slot 16 of metadata tag "sidebar" of player to glowing heart of the sea named "&8-&k|&a Lifer Sidebar &8&k|&8-" with lore "&c Sidebar is DISABLED in &aLifer" and "&7 " and "&9  Click to &aenable&9 the sidebar"
		else if {Commence::%player%::LiferBar} is not false:
			set slot 16 of metadata tag "sidebar" of player to glowing heart of the sea named "&8-&k|&a Lifer Sidebar &8&k|&8-" with lore "&a Sidebar is ENABLED in &aLifer" and "&7 " and "&9  Click to &cdisable&9 the sidebar"
		set slot 22 of metadata tag "sidebar" of player to paper named "&8-&k|&9 Suggestions? &8&k|&8-" with lore "&7Want to see something here?" and "&8Type &c/report [text]" and "&7" and "&9  Click for help reporting"
		set slot 8 of metadata tag "sidebar" of player to structure void named "&4-&k|&c &lClick to Close &4&k|&4-"
		open (metadata tag "sidebar" of player) to player

